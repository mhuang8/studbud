CACA,aACA,CAGA,gBACA,qBACA,CACA,UACA,UAAA,CAEA,gBACA,CACA,oBAHA,aAKA,CACA,mBACA,UACA,CACA","sources":["public/style.css"],"sourcesContent":["/*--------------Base Styles--------------*/\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\nhtml {\n\tscroll-behavior: smooth;\n\tfont-size: 100%;\n  height: 100%;\n  background-color: #EEF2F9;\n}\n\nbody {\n  width:100%;\n  height: 100%;\n  max-width: 100%;\n  margin: 0 auto;\n  font-family: 'Lato', sans-serif;\n  color: #242424;\n  padding-left:180px;\n}\n\nsection{\n width:100%;\n height:100%;\n}\n\n#studbud{\n  padding: 5rem 4.5rem;\n  \n\n}\n#tasklist, #kanbanboard, #stopwatch, #flowtime, #resourcelist {\n  padding: 3rem 1.8rem;\n}\n\nmain button{\n  color: white;\n  background-color: #6182EB;\n  border: none;\n  padding: 0.9rem;\n  font-family: 'Nunito', sans-serif;\n  font-weight: 600;\n  border-radius: 30px;\n  cursor: pointer;\n}\n\nmain button:hover{\n  background-color: rgb(77, 109, 214);\n  transition: background-color 0.2s;\n}\n\nul{\n  list-style: none;\n}\n\n/*------Base Input Styles--------*/\ninput,select{\n  padding:15px;\n  height: 50px;\n  margin: 5px;\n  border: 2px solid #9CC2F4;\n  font-size: 0.9rem;\n  border-radius: 10px;\n  box-shadow: 1px 2px 5px 0px rgba(179, 181, 192, 0.1);\n}\n\ntextarea{\n  font-family: 'Lato', sans-serif;\n  padding:15px;\n  height: 80px;\n  margin: 5px;\n  border: 2px solid #9CC2F4;\n  font-size: 0.9rem;\n  border-radius: 10px;\n  box-shadow: 1px 2px 5px 0px rgba(179, 181, 192, 0.1);\n\n}\n\ninput:focus, select:focus,textarea:focus{\n  outline: none;\n  border-radius: 10px;\n  border: 2px solid rgb(77, 109, 214);\n}\n\nselect:invalid { color: rgb(117, 117, 117); }\n\n\n/*--------------Typography--------------*/\nh1 {\n  font-weight: 800;\n  font-size: 3rem;\n  padding:0.3rem 0;\n}\n\nh2 {\n  font-weight: 800;\n  font-size: 1.9rem;\n  margin-top: 0.5rem;\n  color: #6182EB;\n}\n\nh3{\n  font-weight: 800;\n  font-size: 1.5rem;\n  padding: 0.9rem 0;\n}\n\nh4{\n  font-weight: 600;\n  font-size: 1.2rem;\n  padding: 0.6rem 0;\n}\n\np {\n  font-size: 1rem;\n  padding: 0.3rem 0;\n  color: #383839;\n}\n\n\nnav i{\n  font-size: 1rem;\n  padding: 0 0.3rem;\n  \n}\n\n.fa-columns{\n  font-size: 0.9rem;\n}\n\n.fa-toolbox{\n  font-size: 0.9rem;\n  transform: translate(0, -5%);\n}\n\n#studbud .fa-toolbox{\n  font-size: 2.9rem;\n}\n\n/*----------------LAYOUT--------------*/\n.page-container:not(#tasklist){\n  display:none;\n}\n\n/*---------------Navigation---------------*/\nnav.horizontal-nav{\n  width:100%;\n  height: 2.2rem;\n  background-color: #6182EB;\n  box-shadow: 0px 3px 10px 0px rgba(9, 9, 14, 0.15);\n  position: fixed;\n  left: 0;\n  top: 0;\n  z-index: 1;\n}\n.horizontal-nav-links{\n  display:flex;\n  justify-content: space-between;\n}\n.horizontal-nav-links li{\n  padding: 0.45rem 1.6rem;\n  \n}\n\n.horizontal-nav-links a{\n  color: #FFFFFF;\n  text-decoration: none;\n  font-size: 0.9rem;\n  font-weight: 700;\n}\n\n.horizontal-nav-links a:hover{\n  color: rgb(227, 234, 245);\n}\n\n\nnav.vertical-nav{\n  width: 180px;\n  height: 100%;\n  background-color: #383839;\n  display:flex;\n  flex-direction: row;\n  position: fixed;\n  left: 0;\n  top: 0;\n  padding-top:5rem;\n}\n\nul.nav-links{\n  flex-direction: column;\n}\n\n.nav-links li{\n  padding: 1rem 1.6rem;\n}\n\n.nav-links a{\n  color: #FFFFFF;\n  text-decoration: none;\n  font-size: 0.9rem;\n}\n.nav-links a:hover{\n  color: rgb(190, 202, 221);\n}\n\n.nav-links a.active{\n  font-weight:bold;\n  color: #6C9FF4;\n}\n/*-----------------Home Page-----------------*/\nsection#home{\n  padding: 30px 40px;\n}\n\n#introduction-text{\n  width:80%;\n  font-size: 1.2rem;\n  line-height: 1.6rem;\n}\n\n#icon-container p{\n  line-height: 1.4rem;\n}\n\n#icon-container {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.icon-col{\n  width: 25%;\n}\n\n.icon-box{\n  background-color:#FFFFFF;\n  border-radius: 1.2rem;\n  padding: 1rem;\n  margin: 3rem 0 0 0 ;\n  width: 11rem;\n  height: 10rem;\n  display:flex;\n  justify-content: center;\n  align-items: center;\n\n}\n\n/*-----------------Task list-----------------*/\n\ndiv#taskform-box{\n  background-color: rgba(255, 255, 255, 0.95);\n  box-shadow: 0px 4px 10px 0px rgba(106, 115, 151, 0.1);\n  border-radius: 1rem;\n  padding:1.9rem 2.5rem;\n}\n\n#taskform{\n  display:flex;\n  flex-wrap: wrap;\n  width:100%;\n}\n\n/**---individual input boxes---**/\nform #taskInput{\n  flex:4;\n  flex-basis:35%;\n}\n\nform #tasklistButton{\n  font-size: 1rem;\n  text-align: center;\n  height: 50px;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 30px;\n}\n\n/**---html content added through js---**/\n/* #tasklist-labels{\n  padding: 0px 15px;\n  display:flex;\n  width: 100%;\n  color: rgb(104, 110, 129);\n}\n\n#tasklist-labels h3:first-child{\n  flex-basis:40%;\n\n}\n#tasklist-labels h3{\n  flex-basis:14%;\n}\n/**---*/\ntable{\n  margin-top: 10px;\n  border-spacing: 0 10px; \n  border-collapse: separate; \n  width:100%;\n}\n\n#tasklistTable th{\n  color: rgb(92, 97, 114);\n}\n\n\n #tasklistTable th, #tasklistTable td {\n  padding: 10px 20px;\n  text-align: left;\n  vertical-align: middle;\n}\n\n.newTaskItem{\n  background-color: #E2EDF7;\n}\n\ntd label{\n  display: flex;\n  align-items: center;\n}\n\ntd{\n  border: solid 1px transparent;\n  border-style: solid none;\n}\n.dueDate {\n  display: none;\n}\ntd:first-child{\n  border-left-style: solid;\n  border-top-left-radius: 10px; \n  border-bottom-left-radius: 10px;\n}\n\ntd:last-child {\n  border-right-style: solid;\n  border-bottom-right-radius: 10px; \n  border-top-right-radius: 10px; \n}\n\n.taskCheckboxInput, .taskCheckboxInput:focus{\n  height: 0.5rem;\n  width: 0.5rem;\n  margin-right: 0.8rem;\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  -o-appearance: none;\n  appearance: none;\n  border: 3px solid #859DE6;\n  border-radius: 10px;\n  transition-duration: 0.2s;\n  background-color: #FFFFFF;\n  cursor: pointer;\n}\n\n.taskCheckboxInput:checked{\n  background-color: rgb(214, 214, 214);\n}\n\n\n/*----------------Kanban Board------------------*/\n \n#kanbanForm{\n  padding: 0.2rem 0 0.5rem 0;\n}\n\n#board-container{\n  display:flex;\n  overflow-x: scroll;\n}\n\n.boardColumn{\n  background-color: #d2d7f5;\n  box-shadow: 0px 3px 8px 0px rgba(85, 92, 121, 0.1);\n  border-radius: 0.4rem;\n  padding:0.9rem;\n  margin: 0 0.3rem;\n  flex-basis: 25%;\n  flex-flow: column;\n  min-width: 7.5rem;\n  height: 100%;\n  transition: all 0.2s linear;\n}\n\n.boardColumn:nth-child(2n){\n  background-color: rgb(209, 222, 243, 0.9);\n}\n\n#kanbanCard{\n  background-color: #FFFFFF;\n  padding: 0.9rem;\n  margin: 0.9rem 0;\n  border-radius: 0.4rem;\n  box-shadow: 0px 3px 6px 0px rgba(172, 172, 172, 0.1);\n  cursor: move;\n}\n\n.kanbanPriority{\n  background-color: #5598F5;\n  color:#FFFFFF;\n  width:5rem;\n  padding: 0.1rem 0.5rem;\n  border-radius: 5px;\n}\n\n/*----------------Stopwatch------------------*/\n#stopwatch-box{\n  padding: 3rem;\n  background-color: #ffffff;\n  border-radius: 1rem;\n  box-shadow: 0px 4px 10px 0px rgba(106, 115, 151, 0.1);\n}\n#counter{\n  color: #383839;\n  font-size: 10rem;\n  width: 100%;\n  text-align: center;\n}\n\n#stopwatchButtonsContainer{\n  display: flex;\n  justify-content: center;\n\n}\n#stopwatchButtonsContainer button{\n  width: 10rem;\n  height: 4.5rem;\n  margin: 2rem 1rem;\n  font-size: 1.4rem;\n  border-radius: 3rem;\n}\n\n#startCounter{\n  background-color: #6182EB;\n  color:#ffffff;\n  border: 0.25rem solid #6182EB;\n}\n#startCounter:hover{\n  background-color: #ffffff;\n  color:#6182EB;\n  box-shadow: 1px 3px 6px 0px rgba(85, 95, 156, 0.15);\n  transition: background-color 0.3s, color 0.3s, box-shadow 0.3s;\n}\n\n#stopCounter{\n  background-color: #DA6EA4;\n  color:#ffffff;\n  border: 0.25rem solid #DA6EA4;\n}\n#stopCounter:hover{\n  background-color: #ffffff;\n  color:#DA6EA4;\n  box-shadow: 1px 3px 6px 0px rgba(168, 102, 136, 0.15);\n  transition: background-color 0.3s, color 0.3s, box-shadow 0.3s;\n}\n\n#resetCounter{\n  background-color: #f5ad2a;\n  color:#ffffff;\n  border: 0.25rem solid #f5ad2a;\n}\n#resetCounter:hover{\n  background-color: #ffffff;\n  color:#f5ad2a;\n  box-shadow: 1px 3px 6px 0px rgba(184, 153, 97, 0.15);\n  transition: background-color 0.3s, color 0.3s, box-shadow 0.3s;\n}\n/*----------------Flow Time Tracker------------------*/\n#todayDate{\n  background-color:#a3b2e6;\n  color: #FFFFFF;\n  text-align: center;\n  padding: 0.8rem 1.2rem;\n  border-radius: 0.4rem;\n}\n\n#flowTimeTable{\n  border-spacing: 0 8px; \n}\n\n.flowTimeTask{\n  background-color: #FFFFFF;\n}\n\n#flowTimeTable th{\n  padding: 0.6rem 1.2rem;\n  text-align: left;\n  vertical-align: middle;\n}\n#flowTimeTable td {\n  padding: 1.25rem;\n}\n\n/*----------------Music Player------------------*/\nnav button{\n  background-color: Transparent;\n  border: none;\n  outline:none;\n  overflow: hidden;\n  cursor: pointer;\n}\n\nnav .fa{\n  font-size:1.1rem;\n  color: #ffffff;\n}\n\nnav .fa:hover{\n  color: rgb(227, 234, 245);\n}\n\n/*applies to both music player and resource list */\n.modal-box {\n  display: none; \n  position: fixed; \n  z-index: 1; /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgba(0,0,0,0.3); \n}\n\n.modal-content {\n  background-color: #ffffff;\n  box-shadow: 0px 3px 15px 0px rgba(87, 87, 87, 0.1);\n  margin: 2.7rem 1rem; \n  float: right;\n  padding: 1.7rem;\n  border-radius:1.25rem;\n}\n\n.music-container{\n  display: flex;\n  position: relative;\n}\n\n.img-container{\n  position: relative;\n}\n\n.img-container::after{\n  content: '';\n  background-color:#ffffff;\n  width: 1.5rem;\n  height: 1.5rem;\n  position: absolute;\n  left:50%;\n  bottom: 50%;\n  border-radius: 50%;\n  transform: translate(-50%, 40%);\n}\n.img-container img{\n  width: 8.5rem;\n  height: 8.5rem;\n  border-radius: 50%;\n  object-fit: cover;\n\n  animation: rotate 5s linear infinite;\n  animation-play-state: paused;\n}\n\n.music-container.play .img-container img{\n  animation-play-state: running;\n}\n\n@keyframes rotate{\n  from{\n    transform: rotate(0deg);\n  }\n  to{\n    transform: rotate(360deg);\n  }\n}\n\n.music-container-right{\n  padding: 0 1.5rem;\n  width: 25rem;\n}\n\n#songTitle{\n  padding: 0;\n}\n\n#songArtist{\n  color:rgb(121, 123, 128);\n  font-size: 0.9rem;\n}\n\n.progress-container{\n  background: #e0e0e0;\n  border-radius: 0.3rem;\n  cursor: pointer;\n  margin: 0.6rem 0;\n  height: 0.4rem;\n  width: 100%;\n}\n\n.progress-bar{\n  background-color: rgb(107, 133, 211);\n  border-radius: 0.3rem;\n  height: 100%;\n  width: 0%;\n  transition: width 0.1s linear;\n}\n\n.music-controls{\n  display:flex;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  \n}\n\n.action-btn{\n  background-color: #ffffff;\n  color:rgb(188, 197, 221);\n  font-size: 1.5rem;\n  cursor: pointer;\n  margin: 0 0.9rem;\n  padding: 0.7rem 0.9rem;\n}\n\n.action-btn:hover{\n  background-color: #ffffff;\n  color:rgb(107, 133, 211);\n  transition: color 0.2s;\n}\n\n.action-btn-big{\n  color:rgb(150, 166, 211);\n  font-size: 2rem;\n}\n\n/*---------------------Resource List-----------------------*/\n.resource-modal-content {\n  background-color: #ffffff;\n  box-shadow: 0px 3px 15px 0px rgba(87, 87, 87, 0.1);\n  margin: 12% auto;\n  transform: translate(6%, 0);\n  padding: 1.7rem;\n  border-radius:1rem;\n  width: 50%;\n}\n\n#resourceForm{\n  display:flex;\n  flex-direction: column;\n}\n\n#resourceForm input, \n#resourceForm textarea, \n#resourceForm button{\n  margin: 0.5rem;\n}\n\n\n\n/*---------------------Responsive CSS-----------------------*/\n@media screen and (max-width: 700px) {\n  html{\n    width:100%;\n  }\n\n  body {\n    display: block;\n    min-height: 200px;\n    padding:0px;\n  }\n  section{\n    display:block;\n    text-align: center;\n  }\n  /*nav*/\n  .horizontal-nav-links li:first-child{\n    transform: translate(20%, 0%);\n    margin: 0 auto;\n  }\n\n  /*home*/\n  #icon-container{\n    flex-direction:column;\n  }\n  .icon-col{\n    width:100%;\n    margin: 0 auto;\n    padding-top: 3rem;\n  }\n  .icon-box{\n    margin: 0 auto;\n  }\n  #introduction-text{\n    width:100%;\n  }\n}"],"names":[],"version":3,"file":"index.HASH_REF_a9e7eba873a62039f00f175c12a7a8c4.css.map"}